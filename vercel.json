{
  "version": 2,
  "functions": {
    "app.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron-update",
      "schedule": "0 9 * * *"
    }
  ]
}